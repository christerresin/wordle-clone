<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include('../partials/head'); %>
  </head>
  <body class="Game">
    <header><%- include('../partials/header'); %></header>

    <main>
      <ul class="app__highscores-list">
        <% highscores.forEach((highscore) => { %>
        <li class="app__highscores-item">
          <h3><%= highscore.playerId %></h3>
          <h3><%= highscore.gameDuration %>s</h3>
        </li>
        <li class="app__highscores-item">
          <span
            ><%= highscore.wordLength %>-letter word <%= highscore.uniqueLetters
            === true ? 'with unique letters' : 'with any letters' %></span
          >
        </li>
        <% }) %>
      </ul>
    </main>

    <footer><%- include('../partials/footer'); %></footer>
  </body>
</html>
